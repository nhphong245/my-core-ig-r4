          "type": "boolean",
          "item": [
            {
              "linkId": "hajj-6.3.1.1",
              "text": "Jika Ya, tarikh haid terakhir",
              "type": "date"
            }
          ]
        },
        {
          "linkId": "hajj-6.3.2",
          "text": "Adakah anda menghadapi masalah sakit puan",
          "type": "boolean"
        },
        {
          "linkId": "hajj-6.3.3",
          "text": "Pernahkah anda mengalami rahim / peranakan turun (\"Uterine prolapse\")?",
          "type": "boolean"
        }
      ]
    }
