          "type": "boolean"
        },
        {
          "linkId": "hajj-6.1.10",
          "text": "Leukemia",
          "type": "boolean"
        },
        {
          "linkId": "hajj-6.1.11",
          "text": "Lumpuh / Angin Ahmar / Strok",
          "type": "boolean"
        },
        {
          "linkId": "hajj-6.1.12",
          "text": "Penyakit Mental / Psikiatri",
          "type": "boolean"
        },
        {
          "linkId": "hajj-6.1.13",
          "text": "Demensia / Nyanyuk / mudah lupa",
          "type": "boolean"
        },
        {
          "linkId": "hajj-6.1.14",
          "text": "Sawan / Epilepsi",
          "type": "boolean"
        },
        {
          "linkId": "hajj-6.1.15",
