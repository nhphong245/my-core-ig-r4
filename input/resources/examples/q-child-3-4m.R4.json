    {
      "linkId": "child-3-4m.4",
      "text": "Perkara yang membimbangkan ibu bapa/penjaga",
      "type": "text"
    },
    {
      "linkId": "child-3-4m.5",
      "text": "Senarai Semak Perkembangan Umur 3 bulan (sila semak terlebih dahulu apa yang ibubapa/penjaga tulis): Tandakan pada ruangan yang berkenaan",
      "type": "group",
      "item": [
        {
          "linkId": "child-3-4m.5.1",
          "text": "Leher bayi tegak apabila didukung?",
          "type": "boolean"
        },
        {
          "linkId": "child-3-4m.5.2",
          "text": "Bayi menggerakkan semua anggota dengan baik? (Perhatikan)",
          "type": "boolean"
        },
        {
          "linkId": "child-3-4m.5.3",
          "text": "Bayi berkukur atau mengekek? (cooing/ squealing)",
          "type": "boolean"
        },
        {
          "linkId": "child-3-4m.5.4",
          "text": "Bayi terkejut pada bunyi kuat",
          "type": "boolean"
        },
        {
          "linkId": "child-3-4m.5.5",
          "text": "Bayi membalas dengan senyuman?",
          "type": "boolean"
        },
        {
          "linkId": "child-3-4m.5.6",
          "text": "Bayi memandang muka anda?",
          "type": "boolean"
        }
      ]
    },
    {
      "linkId": "child-3-4m.6",
      "text": "Jika ada 'tidak' pada senarai semak perkembangan umur 3 bulan, rujuk Pegawai Perubatan/FMS/Pakar Pediatrik dan isikan maklumat di bawah.",
      "type": "group",
      "item": [
        {
          "linkId": "child-3-4m.6.1",
          "text": "Tarikh Temujanji",
          "type": "dateTime"
        },
        {
          "linkId": "child-3-4m.6.2",
          "text": "Tempat Rujukan",
          "type": "string"
        },
        {
          "linkId": "child-3-4m.6.3",
          "text": "Tujuan Rujukan",
          "type": "string"
        },
        {
          "linkId": "child-3-4m.6.4",
          "text": "Jika ada 'tidak' pada senarai semak, Jalankan penilaian perkembangan mengguna Carta Denver",
          "type": "boolean"
        },
        {
          "linkId": "child-3-4m.6.5",
          "text": "Jika ada 'tidak' pada senarai semak, Lakukan simulasi awal",
          "type": "boolean"
        }
      ]
    },
    {
      "linkId": "child-3-4m.7",
      "text": "Bimbingan Awal Tandakan selepas nasihat diberi. (Rujuk Panduan Ibubapa/ Penjaga)",
      "type": "group",
      "item": [
        {
          "linkId": "child-3-4m.7.1",
          "text": "Luangkan masa untuk bermain dan berbual dengan anak.",
          "type": "boolean"
        },
        {
          "linkId": "child-3-4m.7.2",
          "text": "Gunakan kerusi keselamatan kanak-kanak yang sesuai",
          "type": "boolean"
        },
