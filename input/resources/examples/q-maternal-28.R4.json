        {
          "linkId": "maternal-28.2.13",
          "code": [
            {
              "code": "maternal-28.2.13"
            }
          ],
          "text": "Puerperal Sepsis: Demam, lokia berbau busuk/ luar biasa",
          "type": "boolean"
        },
        {
          "linkId": "maternal-28.2.14",
          "code": [
            {
              "code": "maternal-28.2.14"
            }
          ],
          "text": "Keinginan ingin mencederakan/membunuh  diri",
          "type": "boolean"
        }
